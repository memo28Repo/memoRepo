{
  "name": "@memo28/service",
  "version": "2.1.6",
  "description": "axios configuration based on decorator pluggable",
  "main": "./lib/index",
  "module": "./dist/index",
  "types": "./dts/index.d.ts",
  "scripts": {
    "build": "pnpm build:cjs && pnpm build:es",
    "build:cjs": "tsc --module commonjs --rootDir ./src --outDir ./lib --experimentalDecorators true ",
    "build:es": "tsc --module es6 --rootDir ./src --outDir ./dist --experimentalDecorators true ",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "files": [
    "./dist",
    "./lib",
    "./dts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "http",
    "axios",
    "vue",
    "react",
    "javascript",
    "typescript"
  ],
  "author": "hooks_28",
  "license": "MIT",
  "devDependencies": {
    "axios": "^1.2.2"
  },
  "peerDependencies": {
    "axios": "^1.2.2"
  },
  "dependencies": {
    "@memo28/utils": "^1.1.8",
    "@types/node": "^18.15.6",
    "reflect-metadata": "^0.1.13"
  }
}
